<?xml version="1.0" encoding="UTF-8"?>

<web-app
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0"
>

	<display-name>web</display-name>

	<context-param>
		<param-name>pass.0</param-name>
		<param-value>identity=local;remote=localhost:81</param-value>
	</context-param>

	<context-param>
		<param-name>pass.1</param-name>
		<param-value>identity=vnc;remote=localhost:5900</param-value>
	</context-param>

	<context-param>
		<param-name>pass.2</param-name>
		<!--
		<param-value>identity=ssh;remote=localhost:22;allowed=127.0.0.1</param-value>
		-->
		<param-value>identity=ssh;remote=localhost:22</param-value>
	</context-param>

</web-app>

